{
  "name": "fv-app",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "webpack serve --mode=development",
    "start:sw": "SERVICE_WORKERS=true webpack serve --mode=development",
    "build": "NODE_ENV=production webpack",
    "test": "jest",
    "buildnserve": "npm run build && serve -s build",
    "serve": "serve -s build",
    "icons:generate": "npx fontello-cli install --config fontello.config.json --css ./src/assets/fonts/css --font ./src/assets/fonts/font",
    "icons:open": "npx fontello-cli open --config fontello.config.json",
    "lintStaged": "lint-staged",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write --ignore-path .gitignore \"**/*.+(js|ts|tsx|json)\"",
    "prepare": "husky"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "lint-staged": {
    "*.{json,css,md}": "prettier --write",
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.js": "eslint --cache --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@bcgov/bc-sans": "^2.1.0",
    "@heroicons/react": "^2.2.0",
    "@mothertongues/search": "^0.0.3",
    "@popperjs/core": "^2.11.8",
    "axios": "^1.8.2",
    "classnames": "^2.5.1",
    "core-js": "^3.42.0",
    "dompurify": "^3.2.5",
    "eslint-plugin-react-hooks": "^5.0.0",
    "react": "^18.3.1",
    "react-click-away-listener": "^2.4.0",
    "react-dom": "^18.3.1",
    "react-popper": "^2.3.0",
    "react-router": "^7.6.0",
    "regenerator-runtime": "0.14.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "webpack-manifest-plugin": "^5.0.0",
    "workbox-background-sync": "^7.1.0",
    "workbox-broadcast-update": "^7.1.0",
    "workbox-cacheable-response": "^7.1.0",
    "workbox-core": "^7.1.0",
    "workbox-expiration": "^7.1.0",
    "workbox-google-analytics": "^7.1.0",
    "workbox-navigation-preload": "^7.1.0",
    "workbox-precaching": "^7.1.0",
    "workbox-range-requests": "^7.1.0",
    "workbox-routing": "^7.1.0",
    "workbox-strategies": "^7.3.0",
    "workbox-streams": "^7.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/eslint-parser": "^7.27.1",
    "@babel/eslint-plugin": "^7.27.1",
    "@babel/preset-env": "^7.25.8",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.26.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@eslint/compat": "^1.2.9",
    "@eslint/js": "^9.20.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.33.1",
    "babel-jest": "^29.7.0",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "dotenv": "^16.4.7",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.4",
    "file-loader": "^6.2.0",
    "glob": "^11.0.2",
    "globals": "^16.1.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.4.3",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.3.3",
    "react-refresh": "^0.16.0",
    "serve": "^14.2.4",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.15",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "typescript-eslint": "^8.32.1",
    "uuid": "^11.0.3",
    "webpack": "^5.97.1",
